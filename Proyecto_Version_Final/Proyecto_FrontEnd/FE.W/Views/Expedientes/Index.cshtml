            <td>
                @Html.DisplayFor(modelItem => item.AnteQuirurgicos)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Fracturas)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AnteFamiliares)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CedulaNavigation.Cedula)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdExpediente">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdExpediente">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdExpediente">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
