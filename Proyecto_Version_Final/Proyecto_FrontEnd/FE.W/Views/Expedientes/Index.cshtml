                @Html.DisplayNameFor(model => model.AnteQuirurgicos)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Fracturas)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AnteFamiliares)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CedulaNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FechaN)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Ciudad)
